//--
var Dog = {
  type: 'dog',
  action: 'barks',
  speak: function() { return this.type + ' ' + this.action }
};

console.log(Dog.speak());

var speech = Dog.speak
console.log(speech());

var Cat = {
  type: 'cat',
  action: 'meows'
}

Cat.say = speech
console.log(Cat.say());
